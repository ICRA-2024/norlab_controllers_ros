<launch>
    <node pkg="norlab_controllers_ros" exec="controller_node" name="controller_node" output="screen">
        <remap from="odom_in" to="icp_odom"/>
        <remap from="cmd_vel_out" to="nav_vel"/>
        <param name="controller_config" value="$(find-pkg-share norlab_controllers_ros)/marmotte-ideal-diff-drive-mpc.yaml"/>
        <param name="rotation_controller_config" value="$(find-pkg-share norlab_controllers_ros)/marmotte-rotation-p.yaml"/>
    </node>
</launch>
